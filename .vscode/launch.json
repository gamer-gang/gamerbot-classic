{
  "configurations": [
    {
      "name": "dev",
      "request": "launch",
      "type": "node",
      "program": "${workspaceFolder}/dist/main.js",
      "sourceMaps": true,
      "sourceMapPathOverrides": {
        "webpack:///./*": "${workspaceFolder}/*"
      },
      "outFiles": [
        "${workspaceFolder}/dist/**/*.js"
      ],
      "env": {
        "NODE_ENV": "development"
      }
    }
  ]
}
